func testMathFunctions() -> i32 {
    say("=== Testing xystd Math Functions ===");
    say("");
    
    let x: f64 = 16.0;
    let y: f64 = 2.0;
    
    let sqrtResult: f64 = xy_sqrt(x);
    say("sqrt(16.0) = ");
    xy_say_f64(sqrtResult);
    say("");
    
    let powResult: f64 = xy_pow(y, 3.0);
    say("pow(2.0, 3.0) = ");
    xy_say_f64(powResult);
    say("");
    
    let sinResult: f64 = xy_sin(0.5);
    say("sin(0.5) = ");  
    xy_say_f64(sinResult);
    say("");
    
    let cosResult: f64 = xy_cos(0.0);
    say("cos(0.0) = ");
    xy_say_f64(cosResult);
    say("");
    
    let a: i32 = -42;
    let absResult: i32 = xy_abs_i32(a);
    say("abs(-42) = ");
    xy_say_i32(absResult);
    say("");
    
    let minResult: i32 = xy_min_i32(10, 20);
    say("min(10, 20) = ");
    xy_say_i32(minResult);
    say("");
    
    let maxResult: i32 = xy_max_i32(10, 20);
    say("max(10, 20) = ");
    xy_say_i32(maxResult);
    say("");
    
    return 0;
}

func main() -> i32 {
    testMathFunctions();
    return 0;
}
